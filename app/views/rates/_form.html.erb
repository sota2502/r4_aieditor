<%= form_with(model: [project, action_state, rate], local: true) do |form| %>
  <%= form.hidden_field :action_state_id, value: action_state.id %>
  <tr>
    <td><%= form.text_field :name, class: 'form-control' %></td>
    <td><%= form.text_field :value, class: 'form-control' %></td>
    <td><%= form.submit '送信', class: 'btn btn-primary' %></td>
    <td></td>
  </tr>
<% end %>
