<%= form_with(model: [project, action_state, search], local: true) do |form| %>
  <%= form.hidden_field :action_state_id, value: action_state.id %>
  <tr>
    <td><%= form.text_field :name, class: 'form-control' %></td>
    <td><%= form.number_field :x1, class: 'form-control' %></td>
    <td><%= form.number_field :y1, class: 'form-control' %></td>
    <td><%= form.number_field :x2, class: 'form-control' %></td>
    <td><%= form.number_field :y2, class: 'form-control' %></td>
    <td colspan="2"><%= form.submit '送信', class: 'btn btn-primary' %></td>
  </tr>
<% end %>
