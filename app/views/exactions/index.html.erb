<p id="notice"><%= notice %></p>

<h1>Exactions</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exactions.each do |exaction| %>
      <tr>
        <td><%= exaction.project_id %></td>
        <td><%= exaction.name %></td>
        <td><%= link_to 'Show', exaction %></td>
        <td><%= link_to 'Edit', edit_exaction_path(exaction) %></td>
        <td><%= link_to 'Destroy', exaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exaction', new_exaction_path %>
