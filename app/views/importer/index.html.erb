<% if @notice.present? %>
  <p id="notice" class="alert alert-danger"><%= @notice %></p>
<% end %>

<h1><%= project.name %> / インポート</h1>

<h2>ファイルから</h2>

<%= form_with(url: project_importer_index_path(project), local: true, multipart: true, class: 'form-inline') do |form| %>
  <div class="form-group">
    <%= form.file_field(:file, class: 'form-control') %>
  </div>
  <div class="form-group">
    <%= form.submit "送信", class: 'btn btn-primary' %>
  </div>
<% end %>
